# Environment variables for workload-csharp
SCENARIO_MATRIX_PATH=/app/config/scenario_matrix.yaml
SCENARIO=small_ordered_same_ts
REPORT_NAME=run
WORKLOAD_LOG_DIR=/logs/workload
INFLUX_HOST=influxdb
INFLUX_PORT=8181
INFLUX_DB=benchdb
MEASUREMENTS=cpu_temp,42,pressure,humidity
TAGS=
POINTS=10000000
TIMESTAMP_SPAN_SEC=0
PARALLEL_CLIENTS=4
SERIES_MULTIPLIER=10
# For very large runs prefer a moderate batch and points-per-request to balance memory and throughput
BATCH_SIZE=1000
POINTS_PER_REQUEST=250
DURATION_SEC=0
FLUSH_INTERVAL_MS=1500
USE_GZIP=true
CHUNK_SIZE_BYTES=200000
SEND_CONCURRENCY=4
